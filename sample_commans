### for running master
python -m cluster-middleware.master.main

### for running submit.py
python -m cluster-middleware.master.user_scripts.submit --JobPath /home/fenris/Documents/8th_semester/DistributedSystems/termProject/FinalOne/cluster-middleware/master/job/sample_jobfile


## for running compute node
python -m cluster-middleware.node.__main__ -serverip 127.0.0.1 -selfip 127.0.0.1


## for killing 
sudo kill -9 `sudo lsof -t -i:9001`

python -m cluster-middleware.master.user_scripts.submit --Kill 1